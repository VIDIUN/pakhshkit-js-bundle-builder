{
  "logger": {
    "debugEnabled": 0,
    "logDir": "@LOG_DIR@",
    "logName": "kaltura_bundle_builder.log",
    "accessLogName": "kaltura_bundle_builder_access.log",
    "errorLogName": "kaltura_bundle_builder_error.log"
  },

  "server": {
    "version": "v1.0.0",
    "remote_addr_header_salt": "@APP_REMOTE_ADDR_HEADER_SALT@",
    "remote_addr_header_timeout": 120
  },

  "cloud": {
    "requestTimeout": 100000,
    "httpPort": "@HTTP_PORT@",
    "httpsPort": "@HTTPS_PORT@"
  }

}
